{
  "name": "ryanep.com-frontend",
  "description": "Portfolio website",
  "version": "1.0.1",
  "author": "Ryan Elliott-Potter <ryan@ryanep.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanep/ryanep.com-frontend.git"
  },
  "scripts": {
    "build": "rm -rf ./dist && webpack --config ./config/webpack/webpack.config.prod.babel.js",
    "start": "webpack-dev-server ./src/client --hot --inline --config ./config/webpack/webpack.config.dev.babel.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:js": "eslint .",
    "fmt:css": "stylefmt -r 'src/**/*.scss'",
    "fmt:js": "prettier --write --single-quote 'src/**/*.js'",
    "fmt": "npm run fmt:css && npm run fmt:js",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "compression": "1.7.3",
    "ejs": "2.6.1",
    "express": "4.16.3",
    "helmet": "^3.13.0",
    "isomorphic-fetch": "2.2.1",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-helmet": "5.2.0",
    "react-lazyload": "2.3.0",
    "react-markdown": "3.4.1",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-svg-inline": "2.1.1",
    "redux": "4.0.0",
    "redux-saga": "0.16.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "1.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "21.18.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "7.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.4.2",
    "lint-staged": "7.2.0",
    "node-sass": "4.9.2",
    "nodemon": "1.18.3",
    "prettier": "1.14.0",
    "raw-loader": "0.5.1",
    "react-test-renderer": "16.4.2",
    "sass-loader": "7.1.0",
    "sass-resources-loader": "1.3.3",
    "source-map-support": "0.5.6",
    "style-loader": "0.21.0",
    "stylefmt": "6.0.2",
    "stylelint": "9.4.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-scss": "^3.2.0",
    "url-loader": "1.0.1",
    "webpack": "4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "3.1.5",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "setupFiles": [
      "./config/jest/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^@actions(.*)$": "<rootDir>/src/app/actions$1",
      "^@adapters(.*)$": "<rootDir>/src/app/adapters$1",
      "^@components(.*)$": "<rootDir>/src/app/components$1",
      "^@constants(.*)$": "<rootDir>/src/app/constants$1",
      "^@containers(.*)$": "<rootDir>/src/app/containers$1",
      "^@helpers(.*)$": "<rootDir>/src/app/helpers$1",
      "^@models(.*)$": "<rootDir>/src/app/models$1",
      "^@reducers(.*)$": "<rootDir>/src/app/reducers$1",
      "^@sagas(.*)$": "<rootDir>/src/app/sagas$1",
      "^@services(.*)$": "<rootDir>/src/app/services$1",
      "^@store(.*)$": "<rootDir>/src/app/store$1"
    },
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote",
      "eslint",
      "git add"
    ],
    "*.scss": [
      "stylefmt -r",
      "git add"
    ]
  }
}
