{
  "name": "ryanep.com-frontend",
  "description": "Portfolio website",
  "version": "1.1.0",
  "author": "Ryan Elliott-Potter <ryan@ryanep.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanep/ryanep.com-frontend.git"
  },
  "scripts": {
    "build": "rm -rf ./dist && webpack --config ./config/webpack/webpack.config.prod.babel.js",
    "start": "webpack-dev-server ./src/client --hot --inline --config ./config/webpack/webpack.config.dev.babel.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "npm run lint:js",
    "lint:js": "eslint .",
    "fmt:js": "prettier --write --single-quote 'src/**/*.js'",
    "fmt": "npm run fmt:js",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "compression": "1.7.3",
    "ejs": "2.6.1",
    "express": "4.16.4",
    "helmet": "^3.15.0",
    "isomorphic-fetch": "2.2.1",
    "polished": "^2.3.0",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-ga": "^2.5.3",
    "react-helmet": "5.2.0",
    "react-lazyload": "2.3.0",
    "react-markdown": "4.0.3",
    "react-redux": "5.1.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-svg-inline": "2.1.1",
    "redux": "4.0.1",
    "redux-saga": "0.16.2",
    "styled-components": "^4.1.1",
    "styled-reset": "^1.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "copy-webpack-plugin": "^4.6.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-babel": "5.2.1",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "22.0.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "1.1.4",
    "jest": "23.6.0",
    "lint-staged": "8.0.4",
    "nodemon": "1.18.6",
    "prettier": "1.15.2",
    "raw-loader": "0.5.1",
    "source-map-support": "0.5.9",
    "stylefmt": "6.0.3",
    "stylelint": "9.8.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "url-loader": "1.1.2",
    "webpack": "4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "setupFiles": [
      "./config/jest/"
    ],
    "moduleNameMapper": {
      "^@actions(.*)$": "<rootDir>/src/app/actions$1",
      "^@adapters(.*)$": "<rootDir>/src/app/adapters$1",
      "^@components(.*)$": "<rootDir>/src/app/components$1",
      "^@constants(.*)$": "<rootDir>/src/app/constants$1",
      "^@containers(.*)$": "<rootDir>/src/app/containers$1",
      "^@helpers(.*)$": "<rootDir>/src/app/helpers$1",
      "^@models(.*)$": "<rootDir>/src/app/models$1",
      "^@reducers(.*)$": "<rootDir>/src/app/reducers$1",
      "^@sagas(.*)$": "<rootDir>/src/app/sagas$1",
      "^@services(.*)$": "<rootDir>/src/app/services$1",
      "^@store(.*)$": "<rootDir>/src/app/store$1"
    },
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote",
      "eslint",
      "git add"
    ]
  }
}
